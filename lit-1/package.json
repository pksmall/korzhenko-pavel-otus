{
  "name": "lit-1",
  "version": "1.0.0",
  "description": "Build config for lit-element with Babel and Rollup",
  "main": "src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "copyindex-win": "xcopy /i /q src\\index.html build",
    "copywc-win": "xcopy /i /s /q node_modules\\@webcomponents\\webcomponentsjs\\bundles build && xcopy /i /q node_modules\\@webcomponents\\webcomponentsjs\\webcomponents-loader.js build",
    "build-win": "rmdir /s /q build && mkdir build && npm run copyindex-win && npm run copywc-win && rollup -c",
    "copyindex": "cp src/index.html build",
    "copywc": "cp -r node_modules/@webcomponents/webcomponentsjs/bundles build && cp node_modules/@webcomponents/webcomponentsjs/webcomponents-loader.js build",
    "build": "rm -rf build && mkdir build && npm run copyindex && npm run copywc && rollup -c",
    "start": "serve build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "serve": "^10.1.1"
  },
  "dependencies": {
    "lit-element": "^2.3.1"
  }
}
